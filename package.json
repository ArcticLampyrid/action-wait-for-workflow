{
  "name": "action-wait-for-workflow",
  "version": "1.0.3",
  "private": true,
  "description": "TypeScript template action",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"**/*.ts\"",
    "format-check": "prettier --check \"**/*.ts\"",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ArcticLampyrid/action-wait-for-workflow.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.11.1",
    "@actions/github": "^6.0.1",
    "all": "^0.0.0"
  },
  "devDependencies": {
    "@eslint/compat": "^1.3.1",
    "@types/node": "^24.2.0",
    "@vercel/ncc": "^0.38.3",
    "eslint": "^9.33.0",
    "eslint-plugin-jest": "^29.0.1",
    "jest": "^30.0.5",
    "js-yaml": "^4.1.0",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.1",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.0"
  }
}
